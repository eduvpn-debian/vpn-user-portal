{
    "authors": [
        {
            "email": "fkooman@tuxed.net",
            "name": "Fran\u00e7ois Kooman",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "SURFnet\\VPN\\Portal\\": "src/",
            "SURFnet\\VPN\\Portal\\Tests\\": "tests/"
        }
    },
    "description": "VPN User Portal",
    "license": "AGPL-3.0",
    "name": "eduvpn/vpn-user-portal",
    "repositories": [
        {
            "no-api": true,
            "type": "vcs",
            "url": "https://github.com/eduvpn/vpn-lib-common"
        },
        {
            "no-api": true,
            "type": "vcs",
            "url": "https://github.com/fkooman/php-oauth2-server"
        }
    ],
    "require": {
        "bacon/bacon-qr-code": "^1.0",
        "eduvpn/common": "^1",
        "ext-date": "*",
        "ext-filter": "*",
        "ext-gettext": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-spl": "*",
        "fkooman/oauth2-client": "^5",
        "fkooman/oauth2-server": "^1",
        "fkooman/secookie": "^2",
        "paragonie/constant_time_encoding": "^1",
        "paragonie/random_compat": "^1|^2",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4|^5"
    },
    "suggest": {
        "ext-libsodium": "PHP < 7.2 sodium implementation",
        "ext-sodium": "PHP >= 7.2 sodium implementation"
    }
}
